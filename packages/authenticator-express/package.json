{
  "name": "@authenticator/express",
  "version": "1.0.0",
  "description": "Authenticator For Express.js Server",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build:cjs": "tsc",
    "build:esm": "tsc --outDir ./esm --module es2020",
    "build": "yarn build:esm && yarn build:cjs",
    "clean": "rm -rf esm cjs types"
  },
  "keywords": [
    "auth",
    "server",
    "express"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Youssefgamer685/react-authenticator.git"
  },
  "bugs": {
    "url": "https://github.com/Youssefgamer685/react-authenticator/issues"
  },
  "homepage": "https://github.com/Youssefgamer685/react-authenticator#readme",
  "author": "Youssefgamer685 <ya107231@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "defualt": "./cjs/index.js"
    },
    "./methods": {
      "import": "./esm/methods/index.js",
      "require": "./cjs/methods/index.js",
      "defualt": "./cjs/methods/index.js"
    }
  },
  "files": [
    "./esm/**/*",
    "./cjs/**/*",
    "./types/**/*"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "oauth": "^0.10.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/oauth": "^0.9.1"
  }
}
